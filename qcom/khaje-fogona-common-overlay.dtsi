#include <dt-bindings/interrupt-controller/arm-gic.h>

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/iio/qcom,spmi-vadc.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/clock/qcom,gcc-khaje.h>


#include "khaje-moto-common-overlay.dtsi"

#include "sgm7220.dtsi"
#include "discrete_charging_fogona.dtsi"
#include "discrete-rt-pd-manger.dtsi"

#include "khaje-fogona-sar-sensor-overlay.dtsi"

#include "khaje-fogona-fps-overlay.dtsi"
#include "khaje-fogona-booster.dtsi"
&firmware {
	android {
		vbmeta {
			compatible = "android,vbmeta";
			parts = "vbmeta,boot,system,vendor,dtbo";
		};
	};
};

&pm6125_gpios {
	vib_ldo_en {
		vib_ldo_en_default:vib_ldo_en_default {
			pins = "gpio9";
			function = "normal";
			input-disable;
			power-source = <0>;
			bias-disable;
			output-high;
			qcom,drive-strength = <2>;
		};
                vib_ldo_en_suspend:vib_ldo_en_suspend {
                        pins = "gpio9";
                        function = "normal";
                        input-disable;
                        power-source = <0>;
                        bias-disable;
                        output-low;
                        qcom,drive-strength = <2>;
                };
	};
};

&soc {
    qcom,tz_gpio {
        compatible = "qcom,tz_gpio";
        tlmm-tz = <14>,
                        <15>,
                        <16>,
                        <17>;
    };

	ldo_vib {
		compatible = "moto,vibrator-ldo";
		moto,vib-ldo-gpio = <&pm6125_gpios 9 GPIO_ACTIVE_HIGH>;
		status = "ok";
	};
};

&sdhc_2 {
        vdd-supply = <&L22A>;
        qcom,vdd-voltage-level = <2960000 2960000>;
        qcom,vdd-current-level = <0 800000>;

        vdd-io-supply = <&L5A>;
        qcom,vdd-io-voltage-level = <1800000 2960000>;
        qcom,vdd-io-current-level = <0 22000>;

        vdd-io-bias-supply = <&L7A>;
        qcom,vdd-io-bias-voltage-level = <1256000 1256000>;
        qcom,vdd-io-bias-current-level = <0 6000>;

        pinctrl-names = "default", "sleep";
        pinctrl-0 = <&sdc2_on>;
        pinctrl-1 = <&sdc2_off>;

        cd-gpios = <&tlmm 88 GPIO_ACTIVE_HIGH>;
        status = "ok";
};

&qupv3_se0_i2c {
        status = "ok";
};

&usb0 {
	dwc3@4e00000 {
		dr_mode = "otg";
	};
};




&soc {
        sys_temp_support {
                compatible = "mmi,sys-temp";
                mmi,temperature-names = "front_temp",
                        "back_temp";
                status = "ok";
        };
};

&pm6125_gpios {
	pa_therm2 {
		pa_therm2_default: pa_therm2_default {
			pins = "gpio7";
			bias-high-impedance;
		};
	};
};

&pm6125_vadc {
	pinctrl-0 = <&camera_therm_default &emmc_therm_default &pa_therm2_default>;

	pa_therm2 {
		reg = <ADC5_GPIO4_100K_PU>;
		label = "pa_therm2";
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,pre-scaling = <1 1>;
	};
};

&pm6125_adc_tm {
	pa_therm2 {
		reg = <3>;
		io-channels = <&pm6125_vadc ADC5_GPIO4_100K_PU>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
	};
};

&thermal_zones {
	sdm-skin-therm-usr {
		status = "disabled";
	};
	rear-cam-therm-usr {
		status = "disabled";
	};
	camera-therm-usr {
		status = "disabled";
	};
	quiet-therm-usr {
		status = "disabled";
	};
	pa-therm0-usr {
		status = "disabled";
	};
	pa-therm1-usr {
		status = "disabled";
	};
	xo-therm-usr {
		status = "disabled";
	};

	xo-therm {
		status = "disabled";
	};

	pa-therm0 {
		status = "disabled";
	};

	quiet-therm {
		status = "disabled";
	};

	camera-therm {
		status = "disabled";
	};

	emmc-ufs-therm {
		status = "disabled";
	};

	pa_therm0 {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm 0>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	quiet_therm {
                polling-delay-passive = <0>;
                polling-delay = <0>;
                thermal-governor = "user_space";
                thermal-sensors = <&pm6125_adc_tm 1>;
                wake-capable-sensor;
                trips {
                        active-config0 {
                                temperature = <125000>;
                                hysteresis = <1000>;
                                type = "passive";
                        };
                };
        };

	xo_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm 2>;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	pcm_therm {
                polling-delay-passive = <0>;
                polling-delay = <0>;
                thermal-governor = "user_space";
                thermal-sensors = <&pm6125_adc_tm 3>;
                wake-capable-sensor;
                trips {
                        active-config0 {
                                temperature = <125000>;
                                hysteresis = <1000>;
                                type = "passive";
                        };
                };
        };

	msm_therm {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm_iio 0>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};

	chg_therm  {
		polling-delay-passive = <0>;
		polling-delay = <0>;
		thermal-governor = "user_space";
		thermal-sensors = <&pm6125_adc_tm_iio 1>;
		wake-capable-sensor;
		trips {
			active-config0 {
				temperature = <125000>;
				hysteresis = <1000>;
				type = "passive";
			};
		};
	};


};
